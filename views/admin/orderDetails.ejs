<%- include("../includes/adminheader.ejs") %>
<% orderDetails.forEach(orderDetails=>{ %>
<div class="row m-5">
    <div class="col-12 col-md-6">
        <div class="card">
            <div class="container" style="display: flex; justify-content: center;">
            <img class="card-img-top w-50" src="<%=orderDetails.productData.imageUrl[0].url%>" alt="Unsplash">
            </div>
            <div class="card-header">
                <h5 class="card-title mb-0"><%= orderDetails.productData.title %></h5>
            </div>
            <div class="card-body">
                <p class="card-text"><%= orderDetails.productData.des %></p>
                <p class="card-text"><%= orderDetails.productData.category %></p>
                <p class="card-text"><%= orderDetails.productData.price %></p>
                <a href="#" class="card-link">Card link</a>
                <a href="#" class="card-link">Another link</a>
            </div>
        </div>
    </div>
</div>
<%})%>
    </div>
    </div>


<%- include("../includes/adminheader.ejs") %>
			<main class="content">
				<div class="container-fluid p-0">

					<table class="table table-dark table-striped">
						<thead>
							<tr>
							  <th scope="col">Name</th>
							  <th scope="col">Email</th>
							  <th scope="col">Phone</th>
							  <th scope="col">Status</th>
							</tr>
						  </thead>
						  <tbody>
                            <% users.forEach(usersData=>{ %>
							<tr>
							  <td><%= usersData.username %></td>
							  <td><%= usersData.email %></td>
							  <td><%= usersData.mobile %></td>
							  <td>
                                <% if(usersData.status === true) { %>
                                    <a href="/admin/blockAction/<%=usersData._id%>" class="btn btn-danger">
                                      Block
                                  </a>
                                  <% }else{ %>
                                  <a href="/admin/unBlockAction/<%=usersData._id%>" class="btn btn-success">
                                    Unblock
                                </a>
                                <%}%>
                              </td>
							</tr>
                            <%})%>
						  </tbody>
					  </table>
				</div>
			</main>
		</div>
	</div>

	<script src="../../admin/js/app.js"></script>

<script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.9.2/dist/umd/popper.min.js" integrity="sha384-IQsoLXl5PILFhosVNubq5LC7Qb9DXgDA9i+tQ8Zj3iwWAwPtgFTxbJ8NT4GN1R8p" crossorigin="anonymous"></script>
